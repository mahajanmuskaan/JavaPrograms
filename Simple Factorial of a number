import java.util.*;
class Factorial {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int fact=1;
        if(a==1 || a==0){
            System.out.println("Factorial of : "+a+" is : 1");
        }
        else{
         for(int i =a ;i>=1;i--){
            fact = fact*i;
        }
        System.out.println("Factorial of : "+a+" is : "+fact);   
        }
    }
}
